{
  "appId": "com.capcaptcha.app",
  "appName": "Capacitor Captcha",
  "webDir": "build",
  "bundledWebRuntime": false,
  "plugins": {
    "CapacitorFirebaseAuth": {
      "providers": ["phone"],
      "languageCode": "en",
      "nativeAuth": false,
      "permissions": {
           "google": ["profile", "https://www.googleapis.com/auth/drive"]
       }
    }
  },
  "ios": {
        "contentInset": "always"
  },
  "server": {
    "url": "http://192.168.8.173:3000",
    "cleartext": true
  }
}
